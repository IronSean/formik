{
  "name": "formik",
  "description": "Forms in React, without tears",
  "version": "2.0.1-rc.13",
  "license": "MIT",
  "author": "Jared Palmer <jared@palmer.net>",
  "repository": "jaredpalmer/formik",
  "keywords": [
    "formik",
    "react",
    "react-dom",
    "form",
    "validation",
    "forms",
    "higher order component",
    "hoc"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/formik.umd.production.js",
  "module": "dist/formik.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:watch": "npm run test -- --watchAll",
    "start": "cross-env NODE_ENV=development tsdx watch",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm,umd",
    "prepublish": "npm run build",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001",
    "addc": "all-contributors add",
    "start-website": "cd website && yarn start",
    "build-website": "cd website && yarn install && yarn build",
    "gen-docs": "all-contributors generate && doctoc README.md",
    "size": "size-limit"
  },
  "dependencies": {
    "deepmerge": "^4.1.1",
    "hoist-non-react-statics": "^3.3.0",
    "lodash": "^4.17.14",
    "lodash-es": "^4.17.14",
    "react-fast-compare": "^2.0.1",
    "scheduler": "^0.16.2",
    "tiny-warning": "^1.0.2",
    "tslib": "^1.9.3"
  },
  "optionalDependencies": {},
  "resolutions": {
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-options": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/warning": "^3.0.0",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "all-contributors-cli": "^6.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.0.6",
    "cp-cli": "^2.0.0",
    "cross-env": "6.0.3",
    "doctoc": "^1.3.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.1.2",
    "husky": "3.0.9",
    "just-debounce-it": "^1.1.0",
    "lint-staged": "9.4.2",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0",
    "react-testing-library": "^8.0.1",
    "rimraf": "^3.0.0",
    "tsdx": "^0.9.3",
    "typescript": "^3.5.3",
    "webpack": "^4.31.0",
    "yup": "^0.27.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  },
  "jest": {
    "globals": {
      "__DEV__": "boolean"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "react-testing-library/cleanup-after-each.js",
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "14 kB"
    },
    {
      "path": "./dist/formik.esm.js",
      "limit": "14 kB"
    },
    {
      "path": "./dist/formik.umd.production.js",
      "limit": "14 kB"
    }
  ]
}
